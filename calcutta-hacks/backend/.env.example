# Alchemy API Key - Get from https://dashboard.alchemy.com/
ALCHEMY_API_KEY=your_alchemy_api_key_here

# MetaMask Private Key - Export from MetaMask (Settings > Security & Privacy > Export Private Key)
# WARNING: Never commit your actual private key to version control!
PRIVATE_KEY=your_metamask_private_key_here

# Etherscan API Key - Get from https://etherscan.io/myapikey (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Optional: Enable gas reporting
REPORT_GAS=false
